./src.ts/wallet/json-crowdsale.ts","name":null,"original":{"line":73,"column":13},"generated":{"line":61,"column":4}},{"source":"../../src.ts/wallet/json-crowdsale.ts","name":null,"original":{"line":73,"column":20},"generated":{"line":61,"column":11}},{"source":"../../src.ts/wallet/json-crowdsale.ts","name":null,"original":{"line":73,"column":22},"generated":{"line":62,"column":4}},{"source":"../../src.ts/wallet/json-crowdsale.ts","name":null,"original":{"line":73,"column":32},"generated":{"line":62,"column":14}},{"source":"../../src.ts/wallet/json-crowdsale.ts","name":null,"original":{"line":73,"column":34},"generated":{"line":62,"column":16}},{"source":"../../src.ts/wallet/json-crowdsale.ts","name":null,"original":{"line":73,"column":34},"generated":{"line":62,"column":20}},{"source":"../../src.ts/wallet/json-crowdsale.ts","name":null,"original":{"line":73,"column":36},"generated":{"line":62,"column":30}},{"source":"../../src.ts/wallet/json-crowdsale.ts","name":null,"original":{"line":73,"column":37},"generated":{"line":62,"column":32}},{"source":"../../src.ts/wallet/json-crowdsale.ts","name":null,"original":{"line":73,"column":44},"generated":{"line":62,"column":39}},{"source":"../../src.ts/wallet/json-crowdsale.ts","name":null,"original":{"line":73,"column":45},"generated":{"line":63,"column":2}},{"source":"../../src.ts/wallet/json-crowdsale.ts","name":null,"original":{"line":73,"column":47},"generated":{"line":63,"column":3}},{"source":"../../src.ts/wallet/json-crowdsale.ts","name":null,"original":{"line":74,"column":0},"generated":{"line":64,"column":0}}],"sources":{"../../src.ts/wallet/json-crowdsale.ts":"/**\n *  @_subsection: api/wallet:JSON Wallets  [json-wallets]\n */\n\nimport { CBC, pkcs7Strip } from \"aes-js\";\n\nimport { getAddress } from \"../address/index.js\";\nimport { pbkdf2 } from \"../crypto/index.js\";\nimport { id } from \"../hash/index.js\";\nimport { getBytes, assertArgument } from \"../utils/index.js\";\n\nimport { getPassword, looseArrayify, spelunk } from \"./utils.js\";\n\n\n/**\n *  The data stored within a JSON Crowdsale wallet is fairly\n *  minimal.\n */\nexport type CrowdsaleAccount = {\n    privateKey: string;\n    address: string;\n}\n\n/**\n *  Returns true if %%json%% is a valid JSON Crowdsale wallet.\n */\nexport function isCrowdsaleJson(json: string): boolean {\n    try {\n        const data = JSON.parse(json);\n        if (data.encseed) { return true; }\n    } catch (error) { }\n    return false;\n}\n\n// See: https://github.com/ethereum/pyethsaletool\n\n/**\n *  Before Ethereum launched, it was necessary to create a wallet\n *  format for backers to use, which would be used to receive ether\n *  as a reward for contributing to the project.\n *\n *  The [[link-crowdsale]] format is now obsolete, but it is still\n *  useful to support and the additional code is fairly trivial as\n *  all the primitives required are used through core portions of\n *  the library.\n */\nexport function decryptCrowdsaleJson(json: string, _password: string | Uint8Array): CrowdsaleAccount {\n    const data = JSON.parse(json);\n    const password = getPassword(_password);\n\n    // Ethereum Address\n    const address = getAddress(spelunk(data, \"ethaddr:string!\"));\n\n    // Encrypted Seed\n    const encseed = looseArrayify(spelunk(data, \"encseed:string!\"));\n    assertArgument(encseed && (encseed.length % 16) === 0, \"invalid encseed\", \"json\", json);\n\n    const key = getBytes(pbkdf2(password, password, 2000, 32, \"sha256\")).slice(0, 16);\n\n    const iv = encseed.slice(0, 16);\n    const encryptedSeed = encseed.slice(16);\n\n    // Decrypt the seed\n    const aesCbc = new CBC(key, iv);\n    const seed = pkcs7Strip(getBytes(aesCbc.decrypt(encryptedSeed)));\n\n    // This wallet format is weird... Convert the binary encoded hex to a string.\n    let seedHex = \"\";\n    for (let i = 0; i < seed.length; i++) {\n        seedHex += String.fromCharCode(seed[i]);\n    }\n\n    return { address, privateKey: id(seedHex) };\n}\n"},"lineCount":null}},"error":null,"hash":"429f38955d54ebaa5bc426b51d011950","cacheData":{"env":{}}}