n\n    return decode(\n        data.substring(1 + 2 * subsChrs.length),\n        data.substring(1, 1 + 2 * subsChrs.length));\n}\n"},"lineCount":null}},"error":null,"hash":"5d5a85a5cc42b9a498262258d7c2718c","cacheData":{"env":{}}}